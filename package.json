{
  "name": "vtex-utils",
  "version": "1.6.0",
  "description": "A collection of utilities methods for Vtex stores",
  "main": "dist/vtex-utils.common.js",
  "module": "dist/vtex-utils.esm.js",
  "browser": "dist/vtex-utils.js",
  "scripts": {
    "build": "rollup -c",
    "clear": "del-cli dist",
    "compress": "uglifyjs dist/vtex-utils.js -o dist/vtex-utils.min.js -c -m --comments /^!!/",
    "lint": "eslint src --fix",
    "release": "npm run clear && npm run lint && npm run build && npm run compress && npm test",
    "start": "./node_modules/.bin/rollup -c --watch",
    "test": "mocha ./test/*.js -r jsdom-global/register --colors --timeout 20000"
  },
  "repository": "zeindelf/vtex-utils",
  "homepage": "https://github.com/zeindelf/vtex-utils",
  "license": "MIT",
  "author": {
    "name": "Zeindelf",
    "email": "zeindelf@hotmail.com",
    "homepage": "https://www.zeindelf.com"
  },
  "files": [
    "dist",
    "doc",
    "lib"
  ],
  "keywords": [
    "vtex",
    "utils",
    "utilities",
    "helpers",
    "vtex-helpers",
    "vtex-helpers.js",
    "vtex-utils",
    "javascript",
    "js-helpers",
    "js-utilities",
    "js-utils",
    "locate",
    "location",
    "geolocate",
    "geolocation",
    "front-end",
    "web",
    "component",
    "development"
  ],
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "del-cli": "^1.1.0",
    "eslint": "^4.18.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "jsdom": "11.6.2",
    "jsdom-global": "3.0.2",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.56.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^3.3.11",
    "utilify-js": "^0.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.1"
  }
}
